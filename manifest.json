{
    "name": "autopark",
    "version": "0.2.5",
    "description": "Save idle tabs to pinboard.in",
    "permissions": [
        "tabs",
        "bookmarks",
        "storage",
        "https://api.pinboard.in/*",
        "alarms"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "options_page": "options.html",
    "icons": {
        "1024": "favicon-1024.png"
    },
    "browser_action": {
        "default_title": "autopark",
        "default_icon": "favicon-1024.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}
